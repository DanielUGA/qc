#include <string.h>

namespace std {
  /* Standard C library string functions */
  using ::size_t;
  using ::memcpy;
  using ::memmove;
  using ::strcpy;
  using ::strncpy;
  using ::strcat;
  using ::strncat;
  using ::memccpy;
  using ::memcmp;
  using ::strcmp;
  using ::strcoll;
  using ::strncmp;
  using ::strxfrm;
  using ::memchr;
  using ::strchr;
  using ::strcspn;
  using ::strpbrk;
  using ::strrchr;
  using ::strspn;
  using ::strstr;
  using ::strtok;
  using ::memset;
  using ::strerror;
  using ::strlen;
}

